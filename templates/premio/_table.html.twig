<table class="table table-hover m-table m-table--head-separator-brand" id="premio_table">
    <thead>
    <tr>
        <th>
            #
        </th>
        <th>
            Titulo
        </th>
        <th>
            Fecha
        </th>
        <th>
        </th>
    </tr>
    </thead>
    <tbody>
    {% set i=1 %}
    {% for premio in premios %}
        <tr>
            <td>{{ i }}</td>
            <td>{{ premio.id.titulo }}</td>
            <td>{{ premio.id.fechaCaptacion|date('d-m-Y') }}</td>
            <td>
                <ul class="m-nav m-nav--inline m--pull-right">
                    <li class="m-nav__item">
                        <a class="btn btn-secondary btn-sm" href="{{ path('premio_show', { 'id': premio.id.id }) }}">
                            <i class="flaticon-eye"></i> Ver</a>
                    </li>

                    {% if esGestor==true %}
                        <li class="m-nav__item">
                            <a class="btn btn-info btn-sm" href="{{ path('premio_edit', { 'id': premio.id.id }) }}">
                                <i class="flaticon-edit-1"></i> Editar</a>
                        </li>
                        <li class="m-nav__item">
                            <a class="btn btn-danger btn-sm  eliminar_premio"
                               data-href="{{ path('premio_delete', { 'id': premio.id.id }) }}">
                                <i class="flaticon-delete-1"></i> Eliminar</a>
                        </li>
                    {% endif %}
                </ul>
            </td>
        </tr>
        {% set i=1+i %}
    {% endfor %}
    </tbody>
    <tfoot>
    <tr>
        <th>
            #
        </th>
        <th>
            Titulo
        </th>
        <th>
            Fecha
        </th>
        <th>
        </th>
    </tr>
    </tfoot>
</table>
