<div class="modal-dialog modal-full">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">
                {{ title }}
            </h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"></button>
        </div>
        <div>
            <div class="modal-body" id="modal-body">

                <table class="table m-table m-table m-table--head-separator-primary">
                    <thead>
                    <tr>
                        <th>
                            Nombre
                        </th>
                        <th>
                            Cantidad
                        </th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for obj in data %}
                        <tr>
                            <td>{{ obj['nombre'] }}</td>
                            <td>{{ obj['cantidad'] }}</td>
                        </tr>
                    {% endfor %}
                    </tbody>
                    <tfoot>
                    <tr>
                        <th>
                            Nombre
                        </th>
                        <th>
                            Cantidad
                        </th>
                    </tr>
                    </tfoot>
                </table>
            </div>
            <div class="modal-footer">
                <a class="btn btn-sm btn-success exportar_reporte mt-ladda-btn  ladda-button" data-style="expand-right"><i class="flaticon-file"></i>
                    <span class="ladda-label">Exportar</span>
                    <span class="ladda-spinner"></span>
                </a>
                <a class="btn btn-sm btn-metal" data-dismiss="modal"><i class="flaticon-cancel"></i> Cancelar</a>
            </div>
        </div>
    </div>
</div>
